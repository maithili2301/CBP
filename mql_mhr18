MQL<4>help type;
add type NAME [ADD_ITEM {ADD_ITEM}];
  where ADD_ITEM is:
    | icon FILENAME                         |
    | description VALUE                     |
    | attribute NAME {,NAME}                |
    | form NAME {,NAME}                     |
    | method PROG_NAME {,PROG_NAME}         |
    | TRIGGER PROG_NAME [input ARG_STRING]  |
    | derived TYPE_NAME                     |
    | abstract | [true] |                   |
               | false  |
    | composed                              |
    | maybecomposed                         |
    | !hidden | nothidden                   |
    | property NAME [value STRING]          |
    | property NAME to ADMIN [value STRING] |
  where TRIGGER is:
    | trigger EVENT_TYPE TRIGGER_TYPE [suppressinbatch] |
    | trigger PROMOTE_EVENT_TYPE  precommit             |
    | trigger CREATE_EVENT_TYPE   precreate             |
  where TRIGGER_TYPE is:
    | action    |
    | check     |
    | override  |
    | precommit |
  where PROMOTE_EVENT_TYPE is:
    | promote |
    | demote  |
  where CREATE_EVENT_TYPE is:
    | create        |
    | connect       |
    | majorrevision |
    | clone         |
  where EVENT_TYPE is:
    | create                 |
    | copy                   |
    | addinterface           |
    | addownership           |
    | batchclone             |
    | batchrevise            |
    | changevault            |
    | changename             |
    | changeowner            |
    | changepolicy           |
    | changetype             |
    | checkin                |
    | checkout               |
    | connect                |
    | copy                   |
    | create                 |
    | grant                  |
    | delete                 |
    | disconnect             |
    | lock                   |
    | majorrevision          |
    | minorrevision          |
    | implicitcopy           |
    | implicitdelete         |
    | implicitmajorrevision  |
    | implicitminorrevision  |
    | modify                 |
    | modifyattribute        |
    | modifydescription      |
    | removefile             |
    | removeinterface        |
    | removeownership        |
    | reserve                |
    | revoke                 |
    | transaction            |
    | unlock                 |
    | unreserve              |
    | CUSTOMEVENT_NAME       |
  where ADMIN is:
    | TYPE NAME |

copy type SRC_NAME DST_NAME [MOD_ITEM {MOD_ITEM}];

modify type NAME [MOD_ITEM {MOD_ITEM}];
  where MOD_ITEM is:
    | name NAME                                      |
    | description VALUE                              |
    | icon FILENAME                                  |
    | !hidden | nothidden                            |
    | add    | attribute NAME                        |
    |        | form      NAME                        |
    |        | method    PROG_NAME                   |
    |        | TRIGGER  PROG_NAME [input ARG_STRING] |
    |        | property NAME to ADMIN [value STRING] |
    |        | property NAME [value STRING]          |
    | remove | attribute NAME                        |
    |        | form      NAME                        |
    |        | method    PROG_NAME                   |
    |        | TRIGGER                               |
    |        | property NAME to ADMIN                |
    |        | property NAME                         |
    | property NAME to ADMIN [value STRING]          |
    | property NAME [value STRING]                   |
    | derived TYPE_NAME                              |
    | abstract | [true] |                            |
               | false  |

delete type NAME;

icon type NAME [file FILENAME] [dir DIRECTORY] [verbose];

print type NAME [SELECT] [DUMP] [tcl] [output FILENAME];

list type [modified MOD_CLAUSE] [SCOPE] NAME_PATTERN [where WHERE_CLAUSE] [SELECT [DUMP [RECORDSEP]]] [tcl] [output FILENAME];
  where MOD_CLAUSE is:
    | after DATE        |
    | between DATE DATE |

  where SCOPE is:
    | globalonly |
    | tenantonly |

  where SELECT is:
    | select [[!|not]substitute] FIELD_NAME {FIELD_NAME} |

  where FIELD_NAME is:
    SUB_FIELD[.SUB_FIELD{.SUB_FIELD}]

  where SUB_FIELD is:
    | string          |
    | string[string]  |

  where DUMP is:
      dump [SEPARATOR_STRING]

  where RECORDSEP is:
      recordseparator [SEPARATOR_STRING]
